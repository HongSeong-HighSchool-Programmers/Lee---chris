#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <windows.h>
void textcolor(int color_number)
{
      SetConsoleTextAttribute( GetStdHandle( STD_OUTPUT_HANDLE ), color_number);
};

float absolute(float a)
{
	if(a<0) return -a;
	return a;
}
void swap(int *a, int *b){
	int tmp = *a;
	*a = *b;
	*b = tmp;
}
int gnd(int a, int b){
	int tmp;
	if(a==b) return a;
	if(a<b) swap(&a,&b);
	while(b){
		tmp = a;
		a = b;
		b = tmp%b;
	}
	return a;
}
float squareRoot(int a)
{
	float guess=1.0;
	const float eps = 0.00001;
	while(absolute(guess * guess - a) >= eps)
	 guess=(a/guess+guess) / 2.0;
	 return guess;
}
void firstequator(int a,int b){
    float solve;
	printf("일차방정식 = 0의 계수를 각각 입력해주세요\n");
	printf("ax + b = 0의 꼴로 정리해 정수 a,b를 차례대로 입력하세요(예시:1 -2)\n");
	scanf("%d %d",&a,&b);
	solve = (float)b/a;
	solve = -solve;
	printf("1차방정식의 해는 %f\n",solve);
}
void secequator(int a,int b, int c){
    float solve1,solve2; 
	printf("이차방정식 = 0의 계수를 각각 입력해 주세요\n");
	printf("ax^2 + bx + c = 0의 꼴로 정리해 정수 a,b,c를 차례대로 입력하세요(예시:1 2 1)\n"); 
	scanf("%d %d %d",&a,&b,&c);
	if(b*b - 4*a*c>0){
	solve1 = (-b+squareRoot(b*b - 4*a*c)) / (a*2);
	solve2 = (-b-squareRoot(b*b - 4*a*c)) / (a*2);
	printf("이차방정식의 두 실근은 %f %f\n",solve1,solve2);
    }
    else if(b*b - 4*a*c==0){
      	solve1 = (-b / (a*2));
      	printf("이차방정식의 중근은 %f\n",solve1);
	}
	else{
	    solve1 = (-b+squareRoot(absolute(b*b - 4*a*c))) / (a*2);
	    solve2 = (-b-squareRoot(absolute(b*b - 4*a*c))) / (a*2);
	    printf("이차방정식의 두 허근은 %fi %fi\n",solve1,solve2);
	}
}
int Nsquare(int a){
	int n;
	int solve=1;
	printf("%d를 몇번 제곱할지 입력하세요.\n",a);
	scanf("%d",&n);
	for(int i=0; i<n; i++) solve = solve*a;
	printf("%d의 제곱근은 %d\n",a,solve);
}

