    label1:
    printf("사칙연산은 1을, 방정식은 2를 눌러주세요.\n");
    scanf("%d",&tmp);
    if(tmp==1){
	printf("두 정수를 입력해주세요.\n예시)10 20\n");
	scanf("%d %d",&a,&b);
	label4:
	printf("더하기: +\n빼기: -\n나누기: /\n곱하기: *\n최대공약수:G\n최소공배수: M\n제곱근: R\nN제곱: N\n");
	scanf(" %c",&d);
    switch(d){
    	case '+': printf("%d + %d = %d\n",a,b,a+b);
    	break;
    	case '-': printf("%d - %d = %d\n",a,b,a-b);
    	break;
		case '*': printf("%d X %d = %d\n",a,b,a*b);
    	break;
		case '/': printf("%d / %d = %f\n",a,b,(float) a/b);
    	break;
		case 'G': printf("%d와 %d의 최대공약수는 %d\n",a,b,gnd(a,b));
    	break;
    	case 'M': printf("%d와 %d의 최소공배수는 %d\n",a,b,a*b/gnd(a,b));
    	break;
		case 'R': if(a>=0) printf("%d의 제곱근은 %f\n",a,squareRoot(a));
		          else{
		          	printf("%d의 제곱근은 %fi\n",a,squareRoot(-a));
				  }
				  if(b>=0) printf("%d의 제곱근은 %f\n",b,squareRoot(b));
		          else{
		          	printf("%d의 제곱근은 %fi\n",b,squareRoot(-b));
				  }
    	break;
		case 'N': Nsquare(a); Nsquare(b); printf("\n");
		break;
		default: printf("잘못된 입력입니다.\n"); goto label4; 
	}
	printf("처음으로 돌아가시려면 1을, 종료하시려면 0을 눌러주세요.\n");
	scanf("%d",&tmp);
	if(tmp==1) goto label1;
	else if(tmp==0) return 0;
	else{
		printf("잘못된 입력입니다.\n");
		goto label2;
	}
	}
	else if(tmp==2){
		label3:
		printf("1차방정식은 1을, 2차방정식은 2를 눌러주세요\n");
		scanf("%d",&tmp);
		if(tmp==1){
		 firstequator(a,b);
	    }
	    else if(tmp==2){
	    	secequator(a,b,c);
		}
		else{
			printf("잘못된 입력입니다.\n");
			goto label3;
		}
	label2:
	printf("처음으로 돌아가시려면 1을, 종료하시려면 0을 눌러주세요.\n");
	scanf("%d",&tmp);
	if(tmp==1) goto label1;
	else if(tmp==0) return 0;
	else{
		printf("잘못된 입력입니다.\n");
		goto label2;
	}
}
    else{
	   printf("잘못된 입력입니다.\n");
	   goto label1;;	
	} 
}
